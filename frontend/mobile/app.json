{
  "expo": {
    "name": "UniVibe Mobile",
    "slug": "univibe-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "univibe",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101016"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#101016"
      },
      "package": "com.univibe.app"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.univibe.app"
    },
    "plugins": [
      "expo-proximity",
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access motion sensors."
        }
      ],
      "expo-blur",
      "expo-haptics"
    ],
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    }
  }
}
